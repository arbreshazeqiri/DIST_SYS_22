<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="400.0" minWidth="600.0" styleClass="user-list" stylesheets="@../resources/styles/app.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pdg.controllers.WishlistProductController">
    <children>
        <ScrollPane fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" minWidth="200.0" pannable="true" prefWidth="560.0" style="-fx-background-color: transparent;" VBox.vgrow="ALWAYS">
            <content>
                <FlowPane fx:id="productsPane" hgap="15.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="134.0" minWidth="200.0" prefWrapLength="250.0" vgap="15.0" />
            </content>
        </ScrollPane>
        <HBox fx:id="paginationPane" alignment="CENTER" prefHeight="13.0" prefWidth="511.0" spacing="5.0" styleClass="pagination-list">
            <children>
                <Button mnemonicParsing="false" styleClass="wishlistbtn" text="&lt;" />
                <Button mnemonicParsing="false" styleClass="wishlistbtn" text="1" />
                <Button mnemonicParsing="false" styleClass="wishlistbtn" text="2" />
                <Button mnemonicParsing="false" styleClass="wishlistbtn" text="3" />
                <Button mnemonicParsing="false" styleClass="wishlistbtn" text="4" />
                <Label text="...">
                    <padding>
                        <Insets left="8.0" right="8.0" />
                    </padding>
                </Label>
                <Button mnemonicParsing="false" styleClass="wishlistbtn" text="14" />
                <Button mnemonicParsing="false" styleClass="wishlistbtn" text="&gt;" />
            </children>
            <VBox.margin>
                <Insets bottom="-10.0" top="10.0" />
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
